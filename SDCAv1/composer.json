{
	"require" : {
		"cakephp/chronos" : "^2.0",
		"lcobucci/jwt" : "^3.3",
		"php-di/php-di" : "^6.1",
		"ramsey/uuid" : "^3.9",
		"selective/config" : "^0.2.0",
		"slim/http" : "^1.0",
		"slim/psr7" : "^1.0",
		"slim/slim" : "4.*"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^9.1"
	},
	"autoload" : {
		"psr-4" : {
			"App\\" : "src"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"App\\Test\\" : "tests"
		}
	},
	"config" : {
		"process-timeout" : 0,
		"sort-packages" : true
	},
	"name" : "SDCA",
	"description" : "Sistema de gestion contable para autonomos",
	"homepage" : "https://recursosformacion.com/wordpress/?s=sdca",
	"keywords" : [
		"contabilidad",
		"autonomos"
